{
	"name": "log-o-matic",
	"publisher": "tobin",
	"displayName": "log-o-matic 9000",
	"description": "console print tool to make your life easier",
	"version": "1.2.0",
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.87.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tobinP/log-o-matic"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.log",
				"title": "log"
			},
			{
				"command": "extension.deleteLogs",
				"title": "Delete Logs"
			}
		],
		"keybindings": {
			"command": "extension.log",
			"key": "alt+l"
		},
		"configuration": {
			"title": "Log-o-matic",
			"properties": {
				"log-o-matic.useDebug": {
					"type": "boolean",
					"default": false,
					"description": "Use `Debug.WriteLine()` instead of `Console.WriteLine()`"
				},
				"log-o-matic.token": {
					"type": "string",
					"default": "&&&",
					"description": "Choose the character sequence to be used as token identifier"
				},
				"log-o-matic.custom-format": {
					"type": "string",
					"default": "",
					"description": "Make a custom log statement such as `myCustomPrint()"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.20.55",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}